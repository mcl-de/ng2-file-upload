{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": false,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "listFiles": false,
    "noLib": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "./components/**/*.d.ts",
    "./components/**/*.spec.ts",
    "./demo/**/*.d.ts",
    "./demo/**/*.spec.ts",
    "./node_modules"
  ],
  "filesGlob": [
    "./**/*.ts",
    "./typings/index.d.ts",
    "./typings_custom/**/*.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "./components/file-upload/file-drop.directive.ts",
    "./components/file-upload/file-item.class.ts",
    "./components/file-upload/file-like-object.class.ts",
    "./components/file-upload/file-select.directive.ts",
    "./components/file-upload/file-type.class.ts",
    "./components/file-upload/file-upload.module.ts",
    "./components/file-upload/file-uploader.class.ts",
    "./demo/app.component.ts",
    "./demo/components/file-upload-section.ts",
    "./demo/components/file-upload/simple-demo.ts",
    "./demo/components/file-upload/zs-file-demo/demo.ts",
    "./demo/index.ts",
    "./demo/ng2-file-upload-demo.module.ts",
    "./demo/polyfills.ts",
    "./demo/vendor.ts",
    "./ng2-file-upload.d.ts",
    "./ng2-file-upload.ts",
    "./typings_custom/es6-angular-legacy.d.ts",
    "./typings_custom/es6.d.ts",
    "./typings/globals/es2015/index.d.ts",
    "./typings/globals/jasmine/index.d.ts",
    "./typings/globals/require/index.d.ts",
    "./typings/index.d.ts",
    "./typings/modules/webpack/index.d.ts"
  ]
}
